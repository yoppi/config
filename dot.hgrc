[extensions]
color =
fetch =
graphlog =
mq =
pager =
rebase =
record =
transplant =
info = ~/.hgext/info.py
shelve = ~/.hgext/hgshelve.py
histedit = ~/.hgext/hg_histedit.py

[diff]
git = 1

[pager]
pager = LESS='FSRX' less
quiet = True
attend = diff, d, log, l, lc, glog, gl, qdiff, bs

[alias]
l = log
lc = log -b .
gl = glog
st = status .
pr = pull --rebase
b = branch
bs = branches
bsa = branches -a
qi = qimport
qr = qrefresh
qf = qfinish
sh = shelve
ush = unshelve
rec = record
